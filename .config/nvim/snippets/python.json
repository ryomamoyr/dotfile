  "素因数分解": {
    "prefix": "prime",
    "body": [
      "from collections import defaultdict",
      "def prime_factorize(n: int) -> dict:",
      "    \"\"\"prime factorization",
      "    Args:",
      "        n (int): integer",
      "    Returns:",
      "        dict: {prime: count}",
      "    \"\"\"",
      "    res: dict = defaultdict(int)",
      "    while n % 2 == 0:",
      "        res[2] += 1",
      "        n //= 2",
      "    f = 3",
      "    while f * f <= n:",
      "        if n % f == 0:",
      "            res[f] += 1",
      "            n //= f",
      "        else:",
      "            f += 2",
      "    if n != 1:",
      "        res[n] += 1",
      "    return res"
    ]
