  "Dijkstra": {
    "prefix": "dijkstra",
    "body": [
      "from heapq import heappop, heappush",
      "INF = 10**18",
      "def dijkstra(s: int) -> list:",
      "    cost = [INF] * N",
      "    cost[s] = 0",
      "    hq = [(0, s)]",
      "    while hq:",
      "        c, x = heappop(hq)",
      "        if c > cost[x]:",
      "            continue",
      "        for nx, nc in G[x]:",
      "            total = c + nc",
      "            if total >= cost[nx]:",
      "                continue",
      "            cost[nx] = total",
      "            heappush(hq, (total, nx))",
      "    return cost",
      ""
    ]
  },
